<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.inventor">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Inventor"
        tools:targetApi="m">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".All_Items"
            android:label = "All_Items">
        </activity>
        <activity
            android:name=".CameraActivity"
            android:label = "CameraActivity">
        </activity>
        <activity
            android:name=".InventorizationActivity"
            android:label = "InventorizationActivity">
        </activity>
        <activity
            android:name=".ProfileActivity"
            android:label = "ProfileActiviry">
        </activity>
        <activity
            android:name=".EditProfileActivity"
            android:label = "EditProfileActivity">
        </activity>
        <activity
            android:name=".InventorizationActivityStart"
            android:label = "InventorizationActivityStart">
        </activity>
        <activity
            android:name=".ChangeLocationActivity"
            android:label = "ChangeLocationActivity">
        </activity>
        <activity
            android:name=".ChangeOwnerActivity"
            android:label = "ChangeOwnerActivity">
        </activity>
    </application>

    <meta-data android:name="com.google.android.gms.vision.DEPENDENCIES" android:value="barcode"/>

</manifest>